Solution 1
------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
#include<bits/stdc++.h>
using namespace std;

#define ll long long int
#define N 100010
#define LD long double

int inf = 1e9;
int mod = 1e9 + 7;

int main()
{
    //freopen("IN.txt", "r", stdin);
    //freopen("OUT.txt", "w", stdout);

    ios_base::sync_with_stdio(0);
    cin.tie(0); cout.tie(0);

    int t;
    cin >> t;
    while(t--) {
        int n;
        cin >> n;
        int A[n];
        for(int i = 0; i < n; i++) {
            cin >> A[i];
        }
        int B[n];
        for(int i = 0; i < n; i++) {
            cin >> B[i];
        }
        pair<int,int> p[n];
        for(int i = 0; i < n; i++) {
            p[i].first = A[i] + B[i];
            p[i].second = i;
        }
        sort(p, p + n);
        int a = 0, b = 0;
        for(int i = n - 1, j = 0; i >= 0; i--, j ^= 1) {
            if(!j) {
                a += A[p[i].second];
            }
            else {
                b += B[p[i].second];
            }
        }
        if(a > b) {
            cout << "Alice\n";
        }
        else if(a == b) {
            cout << "Tie\n";
        }
        else {
            cout << "Bob\n";
        }
    }
    return 0;
}
