#include<bits/stdc++.h>
using namespace std;
int main(){
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    int t;cin>>t;
    while(t--){
        int n;cin>>n;
        int a[n];int b[n];
        for(int i=0;i<n;i++){cin>>a[i];}
        for(int i=0;i<n;i++){cin>>b[i];}
        pair<int,int>p[n];
        for(int i=0;i<n;i++){
            p[i].first=a[i]+b[i];
            p[i].second=i;
        }
        sort(p,p+n);
        int pa=0,pb=0;
        for(int i=n-1,j=0;i>=0;i--,j^=1){
            if(j==0){
                pa+=a[p[i].second];
            }
            else if(j==1){
                pb+=b[p[i].second];
            }
        }
        if(pa>pb) cout<<"Alice"<<endl;
        else if(pa<pb) cout<<"Bob"<<endl;
        else cout<<"Tie"<<endl;
    }
}
