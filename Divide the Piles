Solution
------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
#include<bits/stdc++.h>
using namespace std;
int main(){
    ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    int t;cin>>t;
    while(t--){
        int n;cin>>n;
        int g=0;
        for(int i=0;i<n;i++){
            int x;cin>>x;
            g=__gcd(g,x);
        }
        if(g>1) cout<<"Yes"<<endl;
        else cout<<"No"<<endl;
    }
    return 0;
}
